{
  "name": "@josephuspaye/open-private",
  "version": "0.1.0",
  "description": "Open a URL in private mode in your default browser from the command line on Windows",
  "main": "dist/main.js",
  "repository": "https://github.com/JosephusPaye/open-private.git",
  "author": "Josephus Paye II",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "os": [
    "win32"
  ],
  "engines": {
    "node": ">=10"
  },
  "bin": {
    "open-private": "dist/main.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"*.{js,json,md}\"",
    "format:check": "prettier --list-different \"src/**/*.ts\" \"*.{js,json,md}\"",
    "test": "echo no automated tests"
  },
  "dependencies": {
    "@josephuspaye/default-browser-win": "^0.1.1",
    "enquirer": "^2.3.6",
    "kleur": "^4.1.3",
    "sade": "^1.7.4"
  },
  "devDependencies": {
    "@types/node": "^14.14.16",
    "@types/sade": "^1.7.2",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true
  }
}
